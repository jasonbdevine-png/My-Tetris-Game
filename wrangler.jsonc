{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "jasons-tetris",
  "compatibility_date": "2024-01-01",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  // D1 Database for leaderboard
  // For production: Run `npx wrangler d1 create jasons-tetris-db` and add the database_id
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "jasons-tetris-db",
      "database_id": "local-placeholder"
    }
  ]
}
